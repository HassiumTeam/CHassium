class TestsStats {
    func new () {
        this.passed = 0;
        this.failed = 0;
        this.tests_failed = [];
    }

    func pass () {
        this.passed += 1;
    }

    func fail (test) {
        this.failed += 1;
        this.tests_failed.push (test);
    }

    func toString () {
        return "Passed: " + this.passed
             + "\nFailed: " + this.failed
             + "\nTests Failed: " + this.tests_failed;
    }
}
