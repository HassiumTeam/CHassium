class TestsStats {
    func new () {
        this.passed = 0;
        this.failed = 0;
        this.tests_failed = [];
    }

    func pass () {
        this.passed += 1;
    }

    func fail (test) {
        this.failed += 1;
        this.tests_failed.push (test);
    }

    func toString () {
        return "AA" + "BB";
    }
}

stats = TestsStats ();

func main () {
    assign_tests ();

    println (stats);
}

