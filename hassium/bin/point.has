class Point {
    some = 2;
    func new (x: number, y: number) {
        self.x = x;
        self.y = y;
    }

    func print () {
        println (self.x);
        println (self.y);
    }

    func addX (i) {
        self.x += i;
    }

    func addY (i) {
        self.y += i;
    }

    func __add__ (p: Point) {
        ret = Point(self.x, self.y);
        ret.addX(p.x);
        ret.addY(p.y);
        return ret;
    }
}