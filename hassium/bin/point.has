class Point {
    func new (x, y) {
        self.x = x;
        self.y = y;
    }

    func print () {
        println (self.x);
        println (self.y);
    }

    func addX (i: number) {
        self.x += i;
    }

    func addY (i: number) {
        self.y += i;
    }

    func __add__ (p: Point): Point {
        ret = Point(self.x, self.y);
        ret.addX(p.x);
        ret.addY(p.y);
        return ret;
    }
}

return { Point };